# Supabase Configuration
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Stripe Configuration (for payment processing)
# Get from: https://dashboard.stripe.com/apikeys
VITE_STRIPE_PUBLIC_KEY=your_stripe_publishable_key_here

# Optional: Sentry for error tracking
# Get from: https://sentry.io/settings/
VITE_SENTRY_DSN=your_sentry_dsn_here

# Optional: Google Analytics
VITE_GA_TRACKING_ID=your_ga_tracking_id_here

# Optional: Environment
VITE_APP_ENV=development

# Optional: API URL (if you have a separate backend)
VITE_API_URL=http://localhost:3000

# Application Settings
VITE_APP_NAME=RSF Fitness
VITE_APP_DOMAIN=https://rsfapp.netlify.app
